<div class="card">
    <div class="card-header">
      <div class="fa fa-user"></div>
      <%= user.email %>
    </div>
    <div class="card-footer">
      Signed up
      <%= time_ago_in_words(user.created_at) %>
      ago
    </div>
    <div class="card-footer">
      <%= 'sign_in_count' %>
      <%= user.sign_in_count %>
      <div class="row"></div>
      <%= 'current_sign_in_at:' %>
      <%= user.current_sign_in_at %>
      <div class="row"></div>
      <%= 'last_sign_in_at' %>
      <%= user.last_sign_in_at %>
      <div class="row"></div>
      <%= 'current_sign_in_ip' %>
      <%= user.current_sign_in_ip %>
      <div class="row"></div>
      <%= 'last_sign_in_ip' %>
      <%= user.last_sign_in_ip %>
    </div>
</div>